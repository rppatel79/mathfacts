      "essential": true,
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http",
          "name": "mathfacts-fe-container-8080-tcp"
        }
      ],
      "environment": [],
      "secrets": [],
      "mountPoints": [],
      "volumesFrom": [],
